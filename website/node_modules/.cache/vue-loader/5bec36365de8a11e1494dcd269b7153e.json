orkGraph.vue","sourceRoot":"src/pages","sourcesContent":["<template>\r\n    <div>\r\n        <canvas width=\"960\" height=\"600\"></canvas>\r\n    </div>\r\n</template>\r\n<script>\r\nimport * as d3 from \"d3\";\r\nimport { scaleLinear, scaleBand } from \"d3-scale\";\r\nimport { max, min } from \"d3-array\";\r\nimport { selectAll } from \"d3-selection\";\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            context: Object,\r\n            width: 500,\r\n            height: 500,\r\n            items: []\r\n        };\r\n    },\r\n    mounted() {\r\n        this.init();\r\n    },\r\n    methods: {\r\n        init() {\r\n            var canvas = document.querySelector(\"canvas\"),\r\n            context = canvas.getContext(\"2d\"),\r\n            width = canvas.width,\r\n            height = canvas.height;\r\n\r\n            var simulation = d3.forceSimulation()\r\n                .force(\"link\", d3.forceLink().id(function(d) { return d.id; }))\r\n                .force(\"charge\", d3.forceManyBody())\r\n                .force(\"center\", d3.forceCenter(width / 2, height / 2));\r\n\r\n\r\n            //d3.json(\"C:\\\\Studium\\\\Angewandte_Informatik\\\\Master\\\\2te_Semester\\\\Seminar\\\\datamining_frequent_itemset\\\\datamining_frequent_itemset\\\\website\\\\src\\\\assets\\\\json\\\\test.json\", function(error, graph) {\r\n            if (error) throw error;\r\n\r\n            simulation\r\n                .nodes(graph.nodes)\r\n                .on(\"tick\", ticked);\r\n\r\n            simulation.force(\"link\")\r\n                .links(graph.links);\r\n\r\n            d3.select(canvas)\r\n                .call(d3.drag()\r\n                    .container(canvas)\r\n                    .subject(dragsubject)\r\n                    .on(\"start\", dragstarted)\r\n                    .on(\"drag\", dragged)\r\n                    .on(\"end\", dragended));\r\n\r\n            function ticked() {\r\n                context.clearRect(0, 0, width, height);\r\n\r\n                context.beginPath();\r\n                graph.links.forEach(drawLink);\r\n                context.strokeStyle = \"#aaa\";\r\n                context.stroke();\r\n\r\n                context.beginPath();\r\n                graph.nodes.forEach(drawNode);\r\n                context.fill();\r\n                context.strokeStyle = \"#fff\";\r\n                context.stroke();\r\n            }\r\n\r\n            function dragsubject() {\r\n                return simulation.find(d3.event.x, d3.event.y);\r\n            }\r\n           // });\r\n            },\r\n            ticked() {\r\n                let context = this.context;\r\n\r\n                context.clearRect(0, 0, this.width, this.height);\r\n\r\n                context.beginPath();\r\n                //graph.links.forEach(drawLink);\r\n                context.strokeStyle = \"#aaa\";\r\n                context.stroke();\r\n\r\n                context.beginPath();\r\n                this.items.nodes.forEach(this.drawNode);\r\n                context.fill();\r\n                context.strokeStyle = \"#fff\";\r\n                context.stroke();\r\n            },\r\n            drawNode(d) {\r\n                console.log(d.x)\r\n                this.context.moveTo(d.x + 3, d.y);\r\n                this.context.arc(d.x, d.y, 3, 0, 2 * Math.PI);\r\n            }\r\n    }\r\n};\r\n</script>\r\n<style scoped></style>\r\n"]}]}